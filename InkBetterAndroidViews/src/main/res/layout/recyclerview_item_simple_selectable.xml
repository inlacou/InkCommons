<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:id="@+id/view"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:padding="@dimen/general_all">
	<TextView
		android:id="@+id/tv"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toStartOf="@+id/iv_cb"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintBottom_toBottomOf="parent"
		tools:text="text"/>
	<ImageView
		android:id="@+id/iv_cb"
		android:layout_width="@dimen/checkbox_normal"
		android:layout_height="@dimen/checkbox_normal"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		tools:src="@drawable/ic_check_box_checked"/>
</androidx.constraintlayout.widget.ConstraintLayout>